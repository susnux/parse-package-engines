{
  "name": "parse-package-engines",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub action for parsing the engine and package manager from a package.json",
  "keywords": [
    "actions",
    "package.json"
  ],
  "homepage": "https://github.com/susnux/parse-package-engines#readme",
  "bugs": {
    "url": "https://github.com/susnux/parse-package-engines/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susnux/parse-package-engines.git"
  },
  "license": "MIT",
  "author": "Ferdinand Thiessen <opensource@fthiessen.de>",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run coverage && npm run build",
    "build": "esbuild --bundle --target=node22 --platform=node --format=esm src/index.ts --outfile=dist/index.js",
    "coverage": "npm run test:coverage && coverage-badges --output ./badges/coverage.svg",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "local-action": "@github/local-action . src/main.js .env",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@github/local-action": "^6.0.0",
    "@nextcloud/eslint-config": "^9.0.0-rc.5",
    "@vitest/coverage-v8": "^3.2.4",
    "coverage-badges-cli": "^2.2.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.36.0",
    "make-coverage-badge": "^1.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  },
  "engines": {
    "node": ">=22"
  }
}
